{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "settings": {
        "react": {
            "version": "16.13"
        }
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": ["error", 4],
        "arrow-parens": "off",
        "comma-dangle": "off",
        "object-curly-spacing": "off",
        "require-jsdoc": "off",
        "react/prop-types": "off"
    },
    "ignorePatterns": ["serviceWorker.js"]
}